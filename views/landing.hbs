    <title>EATIVE</title>
    <link rel="icon"
        href="https://static.wixstatic.com/media/1d595b_08578658b4e340a6869236035a2dc190~mv2.png/v1/fit/w_2500,h_1330,al_c/1d595b_08578658b4e340a6869236035a2dc190~mv2.png"
        type="image/x-icon">
    <style>
        body {



            background-color: #f0f4f7;
            overflow-x: hidden;
        }


        .carousel-item {
            height: 450px;
        }

        .carousel-image-1 {
            background: url(https://images.unsplash.com/photo-1496412705862-e0088f16f791?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60);
            background-size: cover;
        }

        .carousel-image-2 {
            background: url(https://images.unsplash.com/photo-1516750548995-3db798a6f427?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60);
            background-size: cover;
        }

        .carousel-image-3 {
            background: url(https://images.unsplash.com/photo-1524010235651-6318320e3276?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60);
            background-size: cover;
        }

        #home-heading {
            position: relative;
            min-height: 200px;
            background-image: url('https://img.freepik.com/free-photo/food-background-food-concept-with-various-tasty-fresh-ingredients-cooking-italian-food-ingredients-view-from-with-copy-space_1220-1491.jpg?size=626&ext=jpg');

            /* Center and scale the image nicely */
            background-position: center;
            background-repeat: no-repeat;
            background-size: cover;
            width: 100%;
            height: 20vh;
            text-align: center;
            color: #fff;
        }

        .dark-overlay {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.7);
        }

        #gallery {
            background-color: blanchedalmond;
        }

        #main-footer {
            background: #000;
            color: #fff;
        }
    </style>
</head>

<body>


    <!-- SHOWCASE SLIDER -->
    <section id="showcase">
        <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item carousel-image-1 active">
                    <div class="container">
                        <div class="carousel-caption  mb-5">
                            <h1 class="mb-5">
                                <span
                                    style="background-color:rgb(236, 56, 56) ;font-size:80px; border-radius:8px; padding:20px;">
                                    &nbsp;EATIVE&nbsp;
                                </span>
                            </h1>

                            <a href="/restaurants" class="btn btn-lg btn-danger">View All Restaurants</a>
                        </div>
                    </div>
                </div>

                <div class="carousel-item carousel-image-2">
                    <div class="container">
                        <div class="carousel-caption  mb-5">
                            <h1 class="mb-5">
                                <span
                                    style="background-color:rgb(236, 56, 56) ;font-size:80px; border-radius:8px; padding:20px;">
                                    &nbsp;EATIVE&nbsp;
                                </span>
                            </h1>

                            <a href="/restaurants" class="btn btn-primary btn-lg">View Restaurants</a>
                        </div>
                    </div>
                </div>

                <div class="carousel-item carousel-image-3">
                    <div class="container">
                        <div class="carousel-caption   mb-5">
                            <h1 class="mb-5">
                                <span
                                    style="background-color:rgb(236, 56, 56) ;font-size:80px; border-radius:8px; padding:20px;">
                                    &nbsp;EATIVE&nbsp;
                                </span>
                            </h1>

                            <a href="/restaurants" class="btn btn-success btn-lg">View Restaurants</a>
                        </div>
                    </div>
                </div>
            </div>

            <a href="#myCarousel" data-slide="prev" class="carousel-control-prev">
                <span class="carousel-control-prev-icon"></span>
            </a>

            <a href="#myCarousel" data-slide="next" class="carousel-control-next">
                <span class="carousel-control-next-icon"></span>
            </a>
        </div>
    </section>

    <!--HOME ICON SECTION  -->
    <section id="home-icons" class="py-5">
        <div class="container">
            <div class="row">
                <div class="col-md-4 mb-2 text-center">
                    <i class="fas fa-coffee fa-3x mb-2"></i>
                    <h3>Cafe`s & Luxury Dining</h3>

                </div>
                <div class="col-md-4 mb-2 text-center">
                    <i class="fa fa-beer fa-3x mb-2"></i>
                    <h3>Drinks & Night Life</h3>

                </div>
                <div class="col-md-4 mb-2 text-center">
                    <i class="fas fa-cart-plus fa-3x mb-2"></i>
                    <h3>Pocket Friendly Delivery</h3>

                </div>

            </div>
        </div>
    </section>

    <!-- HOME HEADING SECTION -->
    <section id="home-heading" class="p-5">
        <div class="dark-overlay">
            <div class="row">
                <div class="col">
                    <div class="container pt-5">
                        <h1>Are You Ready To Get Started?</h1>
                        <p class="d-none d-md-block">Explore the curated lists of top restaurants, cafes, pubs, and bars
                            in Delhi NCR, based on trends</p>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- INFO SECTION -->
    <section id="info" class="py-3">
        <div class="container">
            <div class="row">
                <div class="col-md-6 align-self-center">
                    <h3>Eative <span
                            style="background-color:gold; color:white; padding:5px;border-radius:5px;">Gold</span></h3>
                    <p>Experience new places in your city every day and enjoy limitless dining privileges!</p>
                    <a href="#" class="btn btn-outline-danger btn-lg">Learn More</a>
                </div>
                <div class="col-md-6">
                    <img src="https://images.unsplash.com/photo-1458576627435-430ad58ead3b?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60"
                        alt="" class="img-fluid">
                </div>
            </div>
        </div>
    </section>


